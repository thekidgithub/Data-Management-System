#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+color-support@1.1.3/node_modules/color-support/bin.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+color-support@1.1.3/node_modules/color-support/bin.js" "$@"
fi
